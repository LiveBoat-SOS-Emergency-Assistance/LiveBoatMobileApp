{
  "expo": {
    "scheme": "liveboatapp",
    "name": "LiveBoat",
    "slug": "liveboatapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/liveboat.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./images/liveboat.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/liveboat.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.liveboatapp"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-font", "expo-router"]
  }
}
